<Window x:Class="YeelightBulbControl.PresetsMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:YeelightBulbControl"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="PresetsMenu" Height="200
        " Width="150">
    <Grid>
        <ListBox x:Name="PresetsListBox" HorizontalAlignment="Center" Height="134" VerticalAlignment="Top" Width="150" d:ItemsSource="{d:SampleData ItemCount=5}" DisplayMemberPath="Name" MouseDoubleClick="PresetsListBox_MouseDoubleClick"/>
        <Button x:Name="LoadPresetButton"  Content="Load" HorizontalAlignment="Center" Height="35" Margin="0,134,0,0" VerticalAlignment="Top" Width="150"/>

    </Grid>
</Window>
